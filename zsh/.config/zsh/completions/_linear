# shellcheck disable=SC2154
(( $+functions[_linear_issue_list] )) || _linear_issue_list() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help)'{*-s,--state}'[Filter by issue state (can be repeated for multiple states)]:state:->state-state' \
    '(-h --help --all-states)'--all-states'[Show issues from all states]' \
    '(-h --help --assignee)'--assignee'[Filter by assignee (username)]:assignee:->assignee-string' \
    '(-h --help -A --all-assignees)'{-A,--all-assignees}'[Show issues for all assignees]' \
    '(-h --help -U --unassigned)'{-U,--unassigned}'[Show only unassigned issues]' \
    '(-h --help --sort)'--sort'[Sort order (can also be set via LINEAR_ISSUE_SORT)]:sort:->sort-sort' \
    '(-h --help --team)'--team'[Team to list issues for (if not your default team)]:team:->team-string' \
    '(-h --help --limit)'--limit'[Maximum number of issues to fetch (default: 50, use 0 for unlimited)]:limit:->limit-number' \
    '(-h --help -w --web)'{-w,--web}'[Open in web browser]' \
    '(-h --help -a --app)'{-a,--app}'[Open in Linear.app]' \
    '(-h --help --no-pager)'--no-pager'[Disable automatic paging for long output]'

  case "$state" in
    state-state) __linear_complete state state issue list ;;
    assignee-string) __linear_complete assignee string issue list ;;
    sort-sort) __linear_complete sort sort issue list ;;
    team-string) __linear_complete team string issue list ;;
    limit-number) __linear_complete limit number issue list ;;
  esac
}

