  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_milestone_update] )) || _linear_milestone_update() {

  function _commands() {
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help --name)'--name'[Milestone name]:name:->name-string' \
    '(-h --help --description)'--description'[Milestone description]:description:->description-string' \
    '(-h --help --target-date)'--target-date'[Target date (YYYY-MM-DD)]:date:->date-string' \
    '(-h --help --project)'--project'[Move to a different project]:projectId:->projectId-string' \
    '1:command:_commands'

  case "$state" in
    name-string) __linear_complete name string milestone update ;;
    description-string) __linear_complete description string milestone update ;;
    date-string) __linear_complete date string milestone update ;;
    projectId-string) __linear_complete projectId string milestone update ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_milestone_delete] )) || _linear_milestone_delete() {

  function _commands() {
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -f --force)'{-f,--force}'[Skip confirmation prompt]' \
    '1:command:_commands'
}

# shellcheck disable=SC2154
(( $+functions[_linear_completions] )) || _linear_completions() {

  function _commands() {
    local -a commands
    # shellcheck disable=SC2034
    commands=(
      'bash:Generate shell completions for bash.'
      'fish:Generate shell completions for fish.'
      'zsh:Generate shell completions for zsh.'
    )
    _describe 'command' commands
  }

  function _command_args() {
    case "${words[1]}" in
      bash) _linear_completions_bash ;;
      fish) _linear_completions_fish ;;
      zsh) _linear_completions_zsh ;;
    esac
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '1:command:_commands' \
    '*::sub command:->command_args'

  case "$state" in
    command_args) _command_args ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_completions_bash] )) || _linear_completions_bash() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -n --name)'{-n,--name}'[The name of the main command.]:command-name:->command-name-string'

  case "$state" in
    command-name-string) __linear_complete command-name string completions bash ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_completions_fish] )) || _linear_completions_fish() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -n --name)'{-n,--name}'[The name of the main command.]:command-name:->command-name-string'

  case "$state" in
    command-name-string) __linear_complete command-name string completions fish ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_completions_zsh] )) || _linear_completions_zsh() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -n --name)'{-n,--name}'[The name of the main command.]:command-name:->command-name-string'

  case "$state" in
    command-name-string) __linear_complete command-name string completions zsh ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_config] )) || _linear_config() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]'
}

# shellcheck disable=SC2154
(( $+functions[_linear_schema] )) || _linear_schema() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help --json)'--json'[Output as JSON introspection result instead of SDL]' \
    '(-h --help -o --output)'{-o,--output}'[Write schema to file instead of stdout]:file:->file-string'

  case "$state" in
    file-string) __linear_complete file string schema ;;
  esac
}

# shellcheck disable=SC2154
if [ "${funcstack[1]}" = "_linear" ]; then
  _linear "${@}"
else
  compdef _linear linear;
fi
