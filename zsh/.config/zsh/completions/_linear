# shellcheck disable=SC2154
(( $+functions[_linear_issue_comment] )) || _linear_issue_comment() {

  function _commands() {
    local -a commands
    # shellcheck disable=SC2034
    commands=(
      'add:Add a comment to an issue or reply to a comment'
      'update:Update an existing comment'
      'list:List comments for an issue'
    )
    _describe 'command' commands
  }

  function _command_args() {
    case "${words[1]}" in
      add) _linear_issue_comment_add ;;
      update) _linear_issue_comment_update ;;
      list) _linear_issue_comment_list ;;
    esac
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '1:command:_commands' \
    '*::sub command:->command_args'

  case "$state" in
    command_args) _command_args ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_issue_comment_add] )) || _linear_issue_comment_add() {

  function _commands() {
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -b --body)'{-b,--body}'[Comment body text]:text:->text-string' \
    '(-h --help -p --parent)'{-p,--parent}'[Parent comment ID for replies]:id:->id-string' \
    '1:command:_commands'

  case "$state" in
    text-string) __linear_complete text string issue comment add ;;
    id-string) __linear_complete id string issue comment add ;;
  esac
}

