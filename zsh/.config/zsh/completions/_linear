# shellcheck disable=SC2154
(( $+functions[_linear_issue] )) || _linear_issue() {

  function _commands() {
    local -a commands
    # shellcheck disable=SC2034
    commands=(
      'id:Print the issue based on the current git branch'
      'list:List your issues'
      'title:Print the issue title'
      'start:Start working on an issue'
      'view:View issue details (default) or open in browser/app'
      'url:Print the issue URL'
      'describe:Print the issue title and Linear-issue trailer'
      'commits:Show all commits for a Linear issue (jj only)'
      'pull-request:Create a GitHub pull request with issue details'
      'delete:Delete an issue'
      'create:Create a linear issue'
      'update:Update a linear issue'
      'comment:Manage issue comments'
    )
    _describe 'command' commands
  }

  function _command_args() {
    case "${words[1]}" in
      id) _linear_issue_id ;;
      list) _linear_issue_list ;;
      title) _linear_issue_title ;;
      start) _linear_issue_start ;;
      view|v) _linear_issue_view ;;
      url) _linear_issue_url ;;
      describe) _linear_issue_describe ;;
      commits) _linear_issue_commits ;;
      pull-request|pr) _linear_issue_pull_request ;;
      delete|d) _linear_issue_delete ;;
      create) _linear_issue_create ;;
      update) _linear_issue_update ;;
      comment) _linear_issue_comment ;;
    esac
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '1:command:_commands' \
    '*::sub command:->command_args'

  case "$state" in
    command_args) _command_args ;;
  esac
}

