# shellcheck disable=SC2154
(( $+functions[_linear_issue_create] )) || _linear_issue_create() {

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help --start)'--start'[Start the issue after creation]' \
    '(-h --help -a --assignee)'{-a,--assignee}'[Assign the issue to '"'"'self'"'"' or someone (by username or name)]:assignee:->assignee-string' \
    '(-h --help --due-date)'--due-date'[Due date of the issue]:dueDate:->dueDate-string' \
    '(-h --help -p --parent)'{-p,--parent}'[Parent issue (if any) as a team_number code]:parent:->parent-string' \
    '(-h --help --priority)'--priority'[Priority of the issue (1-4, descending priority)]:priority:->priority-number' \
    '(-h --help --estimate)'--estimate'[Points estimate of the issue]:estimate:->estimate-number' \
    '(-h --help -d --description)'{-d,--description}'[Description of the issue]:description:->description-string' \
    '(-h --help -l --label)'{-l,--label}'[Issue label associated with the issue. May be repeated.]::label:->label-string' \
    '(-h --help --team)'--team'[Team associated with the issue (if not your default team)]:team:->team-string' \
    '(-h --help --project)'--project'[Name of the project with the issue]:project:->project-string' \
    '(-h --help -s --state)'{-s,--state}'[Workflow state for the issue (by name or type)]:state:->state-string' \
    '(-h --help --no-use-default-template)'--no-use-default-template'[Do not use default template for the issue]' \
    '(-h --help --no-color)'--no-color'[Disable colored output]' \
    '(-h --help --no-interactive)'--no-interactive'[Disable interactive prompts]' \
    '(-h --help -t --title)'{-t,--title}'[Title of the issue]:title:->title-string'

  case "$state" in
    assignee-string) __linear_complete assignee string issue create ;;
    dueDate-string) __linear_complete dueDate string issue create ;;
    parent-string) __linear_complete parent string issue create ;;
    priority-number) __linear_complete priority number issue create ;;
    estimate-number) __linear_complete estimate number issue create ;;
    description-string) __linear_complete description string issue create ;;
    label-string) __linear_complete label string issue create ;;
    team-string) __linear_complete team string issue create ;;
    project-string) __linear_complete project string issue create ;;
    state-string) __linear_complete state string issue create ;;
    title-string) __linear_complete title string issue create ;;
  esac
}

