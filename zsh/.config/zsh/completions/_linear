# shellcheck disable=SC2154
(( $+functions[_linear_issue_start] )) || _linear_issue_start() {

  function _commands() {
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(-h --help -A --all-assignees)'{-A,--all-assignees}'[Show issues for all assignees]' \
    '(-h --help -U --unassigned)'{-U,--unassigned}'[Show only unassigned issues]' \
    '(-h --help -f --from-ref)'{-f,--from-ref}'[Git ref to create new branch from]:fromRef:->fromRef-string' \
    '(-h --help -b --branch)'{-b,--branch}'[Custom branch name to use instead of the issue identifier]:branch:->branch-string' \
    '1:command:_commands'

  case "$state" in
    fromRef-string) __linear_complete fromRef string issue start ;;
    branch-string) __linear_complete branch string issue start ;;
  esac
}

