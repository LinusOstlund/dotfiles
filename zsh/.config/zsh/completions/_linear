#compdef linear

# zsh completion support for linear v1.7.0

autoload -U is-at-least

# shellcheck disable=SC2154
(( $+functions[__linear_complete] )) ||
function __linear_complete {
  local name="$1"; shift
  local action="$1"; shift
  integer ret=1
  local -a values
  local expl lines
  _tags "$name"
  while _tags; do
    if _requested "$name"; then
      # shellcheck disable=SC2034
      lines="$(linear completions complete "${action}" "${@}")"
      values=("${(ps:\n:)lines}")
      if (( ${#values[@]} )); then
        while _next_label "$name" expl "$action"; do
          compadd -S '' "${expl[@]}" "${values[@]}"
        done
      fi
    fi
  done
}

# shellcheck disable=SC2154
_linear() {
  local state

  function _commands() {
    local -a commands
    # shellcheck disable=SC2034
    commands=(
      'auth:Manage Linear authentication'
      'issue:Manage Linear issues'
      'team:Manage Linear teams'
      'project:Manage Linear projects'
      'milestone:Manage Linear project milestones'
      'completions:Generate shell completions.'
      'config:Interactively generate .linear.toml configuration'
      'schema:Print the GraphQL schema to stdout'
    )
    _describe 'command' commands
  }

  function _command_args() {
    case "${words[1]}" in
      auth) _linear_auth ;;
      issue|i) _linear_issue ;;
      team|t) _linear_team ;;
      project|p) _linear_project ;;
      milestone|m) _linear_milestone ;;
      completions) _linear_completions ;;
      config) _linear_config ;;
      schema) _linear_schema ;;
    esac
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '(- *)'{-V,--version}'[Show the version number for this program.]' \
    '1:command:_commands' \
    '*::sub command:->command_args'

  case "$state" in
    command_args) _command_args ;;
  esac
}

# shellcheck disable=SC2154
(( $+functions[_linear_auth] )) || _linear_auth() {

  function _commands() {
    local -a commands
    # shellcheck disable=SC2034
    commands=(
      'token:Print the configured API token'
      'whoami:Print information about the authenticated user'
    )
    _describe 'command' commands
  }

  function _command_args() {
    case "${words[1]}" in
      token) _linear_auth_token ;;
      whoami) _linear_auth_whoami ;;
    esac
  }

  _arguments -w -s -S -C \
    '(- *)'{-h,--help}'[Show this help.]' \
    '1:command:_commands' \
    '*::sub command:->command_args'

  case "$state" in
    command_args) _command_args ;;
  esac
}

